        <h6 class="m-0 font-weight-bold text-primary">@Model.Title</h6>
    </div>
    <div class="card-body">
        <div class="chart-area">
            <canvas id="@Model.CanvasId" height="280"></canvas>
        </div>
    </div>
</div>
